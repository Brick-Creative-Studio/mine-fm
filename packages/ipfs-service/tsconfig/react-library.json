{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "React Library",
    "extends": "./base.json",
    "compilerOptions": {
      "esModuleInterop": true,
      "jsx": "react-jsx",
      "lib": ["dom","ES2015"],
      "module": "ESNext",
      "target": "es6",
      "jsxImportSource": "react",
      "resolveJsonModule": true
    }
  }
  